{
  "name": "front-end-rocks-boilerplate",
  "version": "1.2.0",
  "license": "MIT",
  "homepage": "https://github.com/FabienGreard/front-end-rocks-boilerplate#readme",
  "author": "Fabien Gr√©ard <grd.fabien@gmail.com> (http://fabiengreard.com/)",
  "description": "This is an opiniated boilerplate with rocking tools for front-end development",
  "keywords": [
    "boilerplate",
    "typescript",
    "react",
    "react-intl",
    "jest",
    "storybook",
    "testing-library",
    "tailwinds",
    "cypress",
    "nextjs",
    "prettier",
    "eslint",
    "husky",
    "commitlint",
    "standard-version"
  ],
  "scripts": {
    "dev": "next dev",
    "dev:test": "jest --watch",
    "dev:coverage": "jest --coverage --watch",
    "dev:e2e": "cypress run",
    "build": "next build",
    "start": "next start",
    "format": "prettier --write .",
    "lint": "eslint --fix .",
    "test": "jest --silent",
    "coverage": "jest --coverage --silent",
    "e2e": "cypress run --headless",
    "storybook": "start-storybook -p 6006",
    "bump:patch": "standard-version --release-as patch --no-verify",
    "bump:minor": "standard-version --release-as minor --no-verify",
    "bump:major": "standard-version --release-as major --no-verify",
    "clear": "node scripts/clear.js",
    "prepare": "husky install"
  },
  "dependencies": {
    "next": "10.1.3",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-intl": "^5.15.7"
  },
  "devDependencies": {
    "@babel/core": "^7.13.14",
    "@commitlint/cli": "^12.1.1",
    "@commitlint/config-conventional": "^12.1.1",
    "@storybook/addon-essentials": "v6.2.3",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/addon-storyshots": "v6.2.3",
    "@storybook/react": "v6.2.3",
    "@testing-library/cypress": "^7.0.5",
    "@testing-library/jest-dom": "^5.11.10",
    "@testing-library/react": "^11.2.6",
    "@testing-library/react-hooks": "^5.1.1",
    "@types/jest": "^26.0.22",
    "@types/node": "^14.14.37",
    "@types/react": "^17.0.3",
    "@typescript-eslint/eslint-plugin": "^4.21.0",
    "@typescript-eslint/parser": "^4.21.0",
    "autoprefixer": "^10.2.5",
    "babel-loader": "^8.2.2",
    "cypress": "^6.8.0",
    "eslint": "^7.23.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.23.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^6.0.0",
    "isomorphic-fetch": "^3.0.0",
    "jest": "^26.6.3",
    "postcss": "^8.2.9",
    "prettier": "^2.2.1",
    "standard-version": "^9.1.1",
    "tailwindcss": "^2.1.2",
    "typescript": "^4.2.3"
  }
}
